<?xml version="1.0" ?>
<list xmlns="http://schemas.sulu.io/list-builder/list">
    <key>newsletters</key>

    <properties>
        <property name="id" visibility="no" translation="sulu_admin.id">
            <field-name>id</field-name>
            <entity-name>App\Entity\Newsletter\Newsletter</entity-name>
        </property>

        <property name="name" visibility="always" searchability="yes" translation="Titre">
            <field-name>name</field-name>
            <entity-name>App\Entity\Newsletter\Newsletter</entity-name>
        </property>
        <property name="created_at" visibility="always"  type="datetime" translation="Créé le">
            <field-name>created_at</field-name>
            <entity-name>App\Entity\Newsletter\Newsletter</entity-name>
        </property>
        <property name="is_sent" visibility="always" type="bool"  translation="Envoyé">
            <field-name>is_sent</field-name>
            <entity-name>App\Entity\Newsletter\Newsletter</entity-name>
        </property>
        
    </properties>
</list>