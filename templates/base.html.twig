            <script src="https://www.google.com/recaptcha/api.js?render=6Lcco4QpAAAAAOePvPxh5DwGcbeYoCjQY-tfWIxm"></script>

            <script>
                function updateDateTime() {
                    var now = new Date();
                    var dateTimeString = now.toLocaleString(); // Get current date and time as a string
                    document.getElementById('liveDateTime').textContent = dateTimeString;
                }

                // Call updateDateTime function every second to update the time
                setInterval(updateDateTime, 1000);
            </script>    

            <script>
                $(document).ready(function(){
                    $('.mh-1').matchHeight({byRow: false,property: 'height'});
                    $('.mh-2').matchHeight({byRow: false,property: 'height'});
                    $('.mh-3').matchHeight({byRow: false,property: 'height'});
                    $('.mh-4').matchHeight({byRow: false,property: 'height'});
                    $('.mh-5').matchHeight({byRow: false,property: 'height'});
                    $('.mh-6').matchHeight({byRow: false,property: 'height'});

                });

            </script>

            <script>

                const key="6Lcco4QpAAAAAOePvPxh5DwGcbeYoCjQY-tfWIxm";

                $(document).ready(function(){

                    $('#newsletter-form').on("submit", function(e) {
                        e.preventDefault(); // cancel the actual submit

                
                        grecaptcha.ready(function() {
                            grecaptcha.execute(key, {action: 'newletter'}).then(function(token) {
                                document.getElementById("recaptchaNewsletter").value = token;
                                e.currentTarget.submit();
                            });
                        });
                    
                    });
                    $('#contact-form').on("submit", function(e) {
                        e.preventDefault(); 
                        grecaptcha.ready(function() {
                            grecaptcha.execute(key, {action: 'contact'}).then(function(token) {
                                document.getElementById("recaptchaContact").value = token;
                                e.currentTarget.submit();
                            });
                        });
                    
                    });

                });

            </script>

